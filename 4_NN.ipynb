        "def stats(weights):\n",
        "  m = len(y_test)\n",
        "  preds = forward(weights, x_test)\n",
        "  acc = (np.argmax(preds, axis=-1) == y_test).sum() / m * 100\n",
        "  loss = np.sum(- np.log(preds[np.arange(m), y_test])) / m\n",
        "  return {'acc': round(acc, 2), 'loss': round(loss, 2)}"
      ],
